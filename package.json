{
  "name": "robinwkurtz_playground",
  "version": "0.0.1",
  "description": "Frontend for robinwkurtz.com running off WordPress API",
  "main": "start-server.js",
  "engines": {
    "node": "4.4.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node start-server.js",
    "dev:build-server": "webpack --config ./webpack/webpack.config.server.dev.js --watch --colors --display-error-details",
    "dev:build-client": "node ./dev-server.js",
    "dev:run-server": "nodemon ./start-server.js --watch './build/server'",
    "dev": "npm-run-all --print-label --parallel dev:*",
    "prod:build-server": "webpack --config ./webpack/webpack.config.server.prod.js ",
    "prod:build-client": "webpack --config ./webpack/webpack.config.client.prod.js ",
    "prod:run-server": "pm2 start --no-daemon ./start-prod-server.js",
    "prod:start-server": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 start-server.js",
    "prod": "npm run prod:build-server & npm run prod:build-client & npm run prod:run-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinwkurtz/robinwkurtz_playground.git"
  },
  "author": "Robin Kurtz <robinwkurtz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robinwkurtz/robinwkurtz_playground/issues"
  },
  "homepage": "https://github.com/robinwkurtz/robinwkurtz_playground#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "classnames": "^2.2.5",
    "ejs": "^2.4.2",
    "emailjs": "^1.0.8",
    "express": "^4.13.4",
    "fake-style-loader": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "pm2": "^1.1.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^5.0.3",
    "react-redux": "^4.4.5",
    "react-redux-form": "^1.11.0",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "react-transition-group": "^1.1.3",
    "redux": "^3.5.2",
    "redux-connect": "^2.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "redux-ui": "0.0.15"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.23.1",
    "eslint-config-rallycoding": "^3.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.1.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-css-modules": "^4.1.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "universal-webpack": "^0.1.12",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
